<%- include('partial/head') -%>
<ul class="problems">
  <%
    for (let i = 0; i < problems.length; i++) {
    const { id, title, completed_users, difficulty_level, description } = problems[i];
  %>
    <li>
      <h3>
        <a href="/problems/<%=id%>"><%= title %></a>
      </h3>
      <p>Level: <%= difficulty_level %></p>
      <p>Completed: <%= completed_users %></p>
    </li>
  <% } %>
</ul>
<%- include('partial/tail') -%>

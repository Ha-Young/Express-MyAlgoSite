<!DOCTYPE html>
<html>
  <head>
    <title>Codewars</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <ul>
      <% problems.map((problem, index) => { %>
        <li>
          <a href="/problems/<%= problem.id %>">
            <h2><%= problem.title %></h2>
            <h3>난이도 <%= problem.difficulty_level %></h3>
            <h4>정답자 수 <%= problem.completed_users %></h4>
          </a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>

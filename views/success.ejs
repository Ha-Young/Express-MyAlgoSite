<!DOCTYPE html>
<html>
  <head>
    <title>Codewars</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%-include("partials/title"); %>
    <%-include("partials/menu"); %>
    <div id="result-container">
      <h3 id="result-title"><%=message%></h3>
      <ul id="testcase-container">
      <% for (let i = 0; i < testcaseResult.length; i++) { %>
        <% const icon= (testcaseResult[i].passed) ? "o" : "x" %>
        <li id="testcase">
          <div id="solution-container">
            <span>실행 코드 : <%=testcaseResult[i].code%></span>
            <span><%=icon%></span>
            </div>
          <div>기대값 : <%=testcaseResult[i].expected%></div>
          <div>결과값 : <%=testcaseResult[i].result%></div>
        </li>
      <% } %>
      </ul>
      <%- include("partials/back", {originalUrl})%>
    </div>
  </body>
</html>

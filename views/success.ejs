<%- include('partials/problem-detail') %>

<div class="playground success">
  <div class="solution">
    <div class="title">Solution</div>
    <textarea id="codearea" name="usercode"><%= usercode %></textarea>
  </div>
  <div class="output success">
    <div class="title">Output</div>
    <div class="statistics">
      <div class="passed">Passed: <%= results.length %></div>
      <div class="failed">Failed: 0</div>
    </div>
    <div class="results">You have passed all of the tests! :)</div>
  </div>
</div>

<form class="buttons" action="/" method="get">
  <button type="submit" class="code-button">목록으로 돌아가기</button>
</form>

<%- include('partials/codemirror') %>
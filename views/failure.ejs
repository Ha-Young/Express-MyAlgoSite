<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Do+Hyeon&display=swap">
    <link rel='stylesheet' href='/stylesheets/global.css'>
    <link rel='stylesheet' href='/stylesheets/failure.css'>
    <title><%= title %></title>
  </head>
  <body>
    <%- include("./partials/header.ejs") %>
    <div class="fail-result-wrapper">
      <h1> Sorry, It's Failed ðŸ‘»</h1>
      <% if (totalTestcaseResult.length) {%>
        <div class="testcases-wrapper">
          <% for (let testcase of totalTestcaseResult) {%>
            <div class="testcase-container <%= testcase.isPassed ? "passed" : "" %>">
              <div class="testcase-explain">
                <h2><%= testcase.testCode %> expected <%= testcase.testSolution %></h2>
              </div>
              <div class="user-result-explain">
                <h3 class="user-result-text">ðŸ‘‰ Your Result:
                  <%= testcase.userSolution === undefined ? "undefined"
                    : testcase.userSolution === null ? "null"
                      : testcase.userSolution %>
                </h3>
              </div>
            </div>
          <%}%>
        </div>
      <%}%>
      <div class="final-result">
        <h1>Passed <%= passedCaseLength %> / <%= totalTestcaseResult.length %></h1>
      </div>
      <div class="problem-list-link button"><a href="/"><h3>Go back to Problem list</h3></a></div>
    </div>
    <%- include("./partials/footer.ejs") %>
  </body>
</html>

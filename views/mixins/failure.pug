mixin failure(props = {})
  .failure(id=props.id)
    .failureContent__container
      label.failureContent__title You Lose...!
      .failureContent__results
        if (err)
          .result=err
        else
          each result in results
            .result=(`testcase:${result.index} : ${result.message}`)
      .failureContent__button
        button.btn.btn-dark#failureButton try again!

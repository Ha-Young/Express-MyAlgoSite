<div>
  <div class=rotateContent>
    Make
    <div class=rotateFlip>
      <div><div>Code</div></div>
      <div><div>Logic</div></div>
      <div><div>Everything</div></div>
    </div>
    AweSoMe!
  </div>

  <ul class="problem-list">
    <% for(const problem of problems){ %>
      <li class="problem-item">
        <a href="problems/<%= problem.id %>"/>
          <div class="image">
            <img src="<%= problem.img %>" alt="<%= problem.title %>"/>
          </div>
          <div class="item-content">
            <h1><%= problem.title %></h1>
            <div class="tags">
              <div class="tag">
                <svg viewBox="0 0 640 512" title="tags">
                  <path d="M497.941 225.941L286.059 14.059A48 48 0 0 0 252.118 0H48C21.49 0 0 21.49 0 48v204.118a48 48 0 0 0 14.059 33.941l211.882 211.882c18.744 18.745 49.136 18.746 67.882 0l204.118-204.118c18.745-18.745 18.745-49.137 0-67.882zM112 160c-26.51 0-48-21.49-48-48s21.49-48 48-48 48 21.49 48 48-21.49 48-48 48zm513.941 133.823L421.823 497.941c-18.745 18.745-49.137 18.745-67.882 0l-.36-.36L527.64 323.522c16.999-16.999 26.36-39.6 26.36-63.64s-9.362-46.641-26.36-63.64L331.397 0h48.721a48 48 0 0 1 33.941 14.059l211.882 211.882c18.745 18.745 18.745 49.137 0 67.882z" />
                </svg>
                 <%= problem.completed_users %>
              </div>
              <div class="tag">
                <svg viewBox="0 0 448 512" title="calendar">
                  <path d="M12 192h424c6.6 0 12 5.4 12 12v260c0 26.5-21.5 48-48 48H48c-26.5 0-48-21.5-48-48V204c0-6.6 5.4-12 12-12zm436-44v-36c0-26.5-21.5-48-48-48h-48V12c0-6.6-5.4-12-12-12h-40c-6.6 0-12 5.4-12 12v52H160V12c0-6.6-5.4-12-12-12h-40c-6.6 0-12 5.4-12 12v52H48C21.5 64 0 85.5 0 112v36c0 6.6 5.4 12 12 12h424c6.6 0 12-5.4 12-12z" />
                </svg>
                 <%= problem.difficulty_level %>
              </div>
            </div>
          </div>
        </a>
      </li>
     <% } %>
  </ul>
</div>

<div class="form_wrapper">
  <form class="form-common" action="/login" method="POST">
    <h3>Login</h3>
    <h4>input your login information</h4>
    <fieldset>
      <input placeholder="Your Email Address" name="email" type="email" tabindex="2" required>
    </fieldset>
    <fieldset>
      <input placeholder="Your Password" name="password" type="password" tabindex="3" required>
    </fieldset>
    <fieldset>
      <button type="submit">Login</button>
    </fieldset>
    <div class="third-party__container">
      <p class="third-party__reg-option">
        <span class="third-party__line-wrapper"
          ><span class="third-party__line"></span></span
        ><span class="third-party__content"
          ><span class="third-party__or-span">or</span></span
        >
      </p>
      <fieldset class="third-party__google-btn">
        <svg
          viewBox="0 0 24 24"
          width="24px"
          height="24px"
          x="0"
          y="0"
          preserveAspectRatio="xMinYMin meet"
          class="third-party__google-icon"
        >
          <g class="color-icon">
            <path
              style="fill: #e94435"
              d="M12.1,5.8c1.6-0.1,3.1,0.5,4.3,1.6l2.6-2.7c-1.9-1.8-4.4-2.7-6.9-2.7c-3.8,0-7.2,2-9,5.3l3,2.4C7.1,7.2,9.5,5.7,12.1,5.8z"
            ></path>
            <path
              style="fill: #f8bb15"
              d="M5.8,12c0-0.8,0.1-1.6,0.4-2.3l-3-2.4C2.4,8.7,2,10.4,2,12c0,1.6,0.4,3.3,1.1,4.7l3.1-2.4C5.9,13.6,5.8,12.8,5.8,12z"
            ></path>
            <path
              style="fill: #34a751"
              d="M15.8,17.3c-1.2,0.6-2.5,1-3.8,0.9c-2.6,0-4.9-1.5-5.8-3.9l-3.1,2.4C4.9,20,8.3,22.1,12,22c2.5,0.1,4.9-0.8,6.8-2.3L15.8,17.3z"
            ></path>
            <path
              style="fill: #547dbe"
              d="M22,12c0-0.7-0.1-1.3-0.2-2H12v4h6.1v0.2c-0.3,1.3-1.1,2.4-2.2,3.1l3,2.4C21,17.7,22.1,14.9,22,12z"
            ></path>
          </g>
        </svg>
        <a href="/google-login">
          <button name="login_google" type="button" class="google-btn">Login with Google</button>
        </a>
      </fieldset>
    </div>
    <p class="join-info">New to codewar? <a href="/join" title="join">Join now</a></p>
  </form>
</div>
